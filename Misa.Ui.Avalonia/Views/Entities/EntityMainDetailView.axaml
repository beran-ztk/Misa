<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Misa.Ui.Avalonia.Views.Entities.EntityMainDetailView">
    
    <Border>
        <Grid RowDefinitions="40,*">
            <Border Grid.Row="0"
                    BorderThickness="0,0,0,1"
                    BorderBrush="{StaticResource BrushBorder}">
                <Grid ColumnDefinitions="*,*,*,*">
                    <TextBlock Grid.Column="0" Text="Information"  HorizontalAlignment="Center"/>
                    <TextBlock Grid.Column="1" Text="Relations"    HorizontalAlignment="Center"/>
                    <TextBlock Grid.Column="2" Text="Sessions"     HorizontalAlignment="Center"/>
                    <TextBlock Grid.Column="3" Text="History"      HorizontalAlignment="Center"/>
                </Grid>    
            </Border>
            
        </Grid>
    </Border>
</UserControl>
